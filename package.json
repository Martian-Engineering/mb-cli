{
  "name": "mb-cli",
  "version": "0.1.0",
  "type": "module",
  "bin": {
    "mb": "src/mb.ts"
  },
  "scripts": {
    "mb": "bun src/mb.ts",
    "test": "bun test --pass-with-no-tests",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "build": "bun build src/mb.ts --outfile dist/mb.js",
    "build:no-dms": "bun build src/mb.ts --outfile dist/mb.js --define:MB_NO_DMS=true"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "bun-types": "^1.3.8",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3"
  }
}
